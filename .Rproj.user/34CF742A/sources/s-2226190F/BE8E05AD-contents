> library(readr)
orangeec <- read_csv("orangeec.csv")
x=1:10
y=2*x+5
plot(x,y, type="p")
plot(x,y, type="l")
plot(x,y, type="b")
plot(x,y, type="c")
plot(x,y, type="o")
plot(x,y, type="h")
plot(x,y, type="s")
plot(x,y, type="S")
mtcars <- read_csv("mtcars.csv")
View(mtcars)
#Conocer que tipos de variables tenemos
str(mtcars)
#Obtener la documentacion de la variable
?mtcars
#Obtener la clase de la variable
class(mtcars$vs)
  #vs es una variable dicotomica, haremos la correcion
mtcars$vs= as.logical(mtcars$vs)
class(mtcars$vs)
mtcars$vs
#Comprobamos
str(mtcars)

'''
Continuamos viendo orangeec
'''
str(orangeec)
#Obtener datos estadisticos
summary(orangeec)
summary(mtcars)
#convertir el peso
wt=(mtcars$wt*1000/2)
wt
mtcarsnew <- transform(mtcars,wt=mtcars$wt*1000/2)
wt
summary(mtcarsnew)

